{
  "extends": [
    "airbnb",
    "react-app",
    "airbnb/hooks",
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react/jsx-filename-extension": 0,
    "react/prop-types": 0,
    "linebreak-style": 0,
    "no-console": 0,
    "no-unused-vars": "warn"
    "no-console": 0
    "react/prop-types": 0
  }
}
